        require(_msgSender() == ownerAddress || this.isApprovedForAll(ownerAddress, _msgSender()), "You are neither owner nor approved");
        
        transferToken(from, to, tokenId);
    }
    
    function transferToken(address from, address to, uint tokenId) private isTokenOwner(from, tokenId) isNonZeroAddress(to)
    {
        approveForToken(address(0), tokenId);
        
        _balances[from]--;
        _balances[to]++;
        _owners[tokenId] = to;
        
        emit Transfer(from, to, tokenId);
        
        listDelistToken(tokenId, false);

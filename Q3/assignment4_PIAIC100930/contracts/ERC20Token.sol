        _tokenApprovals[tokenId] = to;
        emit Approval(this.ownerOf(tokenId), to, tokenId);
    }
    
    function tokenExists(uint256 tokenId) private view returns (bool) {
        return _owners[tokenId] != address(0);
    }
    
    /********** Start Region: Approve **********/
    


    /********** Start Region: Sale **********/
    
    function startSale() external onlyOwner
    {
        require(!isSaleOngoing(), "Already an ongoing sale");
        
        _saleStartTime = block.timestamp;
        _saleEndTime = _saleStartTime + 30 days;
    }
    
    function stopSale() external onlyOwner
    {
        require(isSaleOngoing(), "No sale to stop");
        
        _saleStartTime = 0;
        _saleEndTime = 0;
    }
    
    function listForSale(uint256 tokenId) external
    {
        address ownerAddress = this.ownerOf(tokenId);
        require(ownerAddress == _msgSender(), "Only owner can list for sale");
        
        listDelistToken(tokenId, true);
    }
    
    function delistFromSale(uint256 tokenId) external
    {
        address ownerAddress = this.ownerOf(tokenId);
        require(ownerAddress == _msgSender(), "Only owner can delist from sale");
        
        listDelistToken(tokenId, false);
    }
    
    function isSaleOngoing() private view returns (bool)
    {
        uint currentTime = block.timestamp;
        return currentTime > _saleStartTime && currentTime < _saleEndTime;
    }
    
    function listDelistToken(uint256 tokenId, bool listToken) private
    {
        _tokensForSale[tokenId] = listToken;
    }
    
    /********** End Region: Sale **********/
    
    
    
    /********** Start Region: Mint **********/
    
    function safeMint(address to) external
    {
        this.safeMint(to, "");
    }
    
    function safeMint(address to, bytes memory data) external
    {
        require(_tokenCurrentSupply < _tokenSupplyCap, "Token limit reached");
        require(to != address(0), "ERC721: mint to the zero address");
        
        _tokenCurrentSupply++;
        uint256 tokenId = _tokenCurrentSupply;
        
        require(!tokenExists(tokenId), "ERC721: token already minted");

        _balances[to]++;
        _owners[tokenId] = to;

        emit Transfer(address(0), to, tokenId);

        require(_checkOnERC721Received(address(0), to, tokenId, data), "ERC721: transfer to non ERC721Receiver implementer");
    }
    
    /********** End Region: Mint **********/
    
    
    
    /********** Start Region: Buy Token **********/
    
    function buyNewToken() payable external isNonZeroAddress(_msgSender())
    {
        require(isSaleOngoing(), "Please wait for sale to begin");
        require(msg.value == _tokenPriceInWei, "Please pay exact Ether amount for token purchase");
        
        this.safeMint(_msgSender());
    }
    
    function buyToken(uint256 tokenId) payable external isNonZeroAddress(_msgSender())
    {
        require(isSaleOngoing(), "Please wait for sale to begin");
        require(msg.value == _tokenPriceInWei, "Please pay exact Ether amount for token purchase");
        
        address ownerAddress = this.ownerOf(tokenId);
        
        require(_tokensForSale[tokenId], "This token is not for sale");
        
        transferToken(ownerAddress, _msgSender(), tokenId);
        
        payable(ownerAddress).transfer(msg.value);
    }
    
    /********** End Region: Buy Token **********/
    
    
    
    /********** Start Region: Token Price **********/
    
    function setTokenPrice(uint tokenPrice) external onlyOwner
    {
        require(tokenPrice > 0, "Token price must be greater than zero");
        _tokenPriceInWei = tokenPrice;
    }
    
    function getTokenPrice() external view returns (uint)
    {
        return _tokenPriceInWei;
    }
    
    /********** End Region: Token Price **********/



    /********** End Region: Base URI **********/
    
    function setBaseUri(string memory baseUri) external onlyOwner
    {
        require(bytes(baseUri).length > 0, "Cannot set empty Base URI");
        _baseURI = baseUri;
    }
    
    function getBaseUri() external view returns (string memory)
    {
        return _baseURI;
    }
    
    /********** End Region: Base URI **********/
    
    
    
    /**
     * @dev Internal function to invoke {IERC721Receiver-onERC721Received} on a target address.
     * The call is not executed if the target address is not a contract.
     *
     * @param from address representing the previous owner of the given token ID
     * @param to target address that will receive the tokens
     * @param tokenId uint256 ID of the token to be transferred
     * @param _data bytes optional data to send along with the call
     * @return bool whether the call correctly returned the expected magic value
     */
    function _checkOnERC721Received(address from, address to, uint256 tokenId, bytes memory _data) private returns (bool) 
    {
        if (to.isContract()) 
        {
            try IERC721Receiver(to).onERC721Received(_msgSender(), from, tokenId, _data) returns (bytes4 retval) 
            {
                return retval == IERC721Receiver.onERC721Received.selector;
            }
            catch (bytes memory reason)
            {
                if (reason.length == 0)
                {
                    revert("ERC721: transfer to non ERC721Receiver implementer");
                } 
                else
                {
                    assembly
                    {
                        revert(add(32, reason), mload(reason))
                    }
                }
            }
        } 
        else
        {
            return true;
        }
    }
}